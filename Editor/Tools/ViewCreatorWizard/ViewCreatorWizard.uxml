                <ui:Toggle label="Has Model" name="toggleSettingHasModel"/>
                <ui:TextField picking-mode="Ignore" label="Name" text="AName" name="inputSettingName"/>
                <ui:TextField picking-mode="Ignore" label="Script Path" name="inputSettingScriptPath">
                    <ui:Button display-tooltip-when-elided="true" name="btnSettingScriptLocation"
                               style="background-image: url(&apos;project://database/Assets/GameFoundation/Editor/Tools/ViewCreatorWizard/folder.png?fileID=2800000&amp;guid=ced5b7b7b9922b5479ed37e0b6b6a90e&amp;type=3#folder&apos;); width: 19px; -unity-background-scale-mode: stretch-to-fill;"/>
                </ui:TextField>
                <ui:TextField picking-mode="Ignore" label="Prefab Path" name="inputSettingPrefabPath">
                    <ui:Button display-tooltip-when-elided="true" name="btnSettingPrefabLocation"
                               style="background-image: url(&apos;project://database/Assets/GameFoundation/Editor/Tools/ViewCreatorWizard/folder.png?fileID=2800000&amp;guid=ced5b7b7b9922b5479ed37e0b6b6a90e&amp;type=3#folder&apos;); width: 19px; -unity-background-scale-mode: stretch-to-fill;"/>
                </ui:TextField>
            </ui:VisualElement>
            <ui:VisualElement class="element-group">
                <ui:Label text="MVP" display-tooltip-when-elided="true" class="label-header"/>
                <ui:TextField picking-mode="Ignore" label="Model" text="Enter a name" name="inputModelName"/>
                <ui:TextField picking-mode="Ignore" label="View" text="Enter a name" name="inputViewName"/>
                <ui:TextField picking-mode="Ignore" label="Presenter" text="Enter a name" name="inputPresenterName"/>
            </ui:VisualElement>
            <ui:Button text="Generate" display-tooltip-when-elided="true" name="btnGenerate"
