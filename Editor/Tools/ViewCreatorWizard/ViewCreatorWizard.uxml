<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance"
         engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd"
         editor-extension-mode="False">
    <Style src="project://database/Assets/GameFoundation/Editor/Tools/ViewCreatorWizard/ViewCreatorWizard.uss?fileID=7433441132597879392&amp;guid=842713300e250954ea51d1849acdb407&amp;type=3#ViewCreatorWizard"/>
    <ui:ScrollView>
        <ui:VisualElement style="padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;">
            <ui:VisualElement name="inputContainer" class="element-group">
                <ui:Label text="Setting" display-tooltip-when-elided="true" class="label-header"/>
